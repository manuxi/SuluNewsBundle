{
    "name": "manuxi/sulu-news-bundle",
    "description": "Say hello to Sulu news!",
    "type": "symfony-bundle",
    "keywords": [
        "sulu",
        "suluCms",
        "sulu bundle",
        "news"
    ],
    "homepage": "https://github.com/manuxi/SuluNewsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Manuxi",
            "email": "manu@bytefresser.de"
        }
    ],
    "require": {
        "php": "^8.1",
        "phpcr/phpcr-migrations-bundle": "^1.6",
        "sulu/sulu": "^2.5",
        "sulu/automation-bundle": "^2.1",
        "symfony/config": "^6.2 | ^7.0",
        "symfony/dependency-injection": "^6.2 | ^7.0",
        "symfony/framework-bundle": "^6.2 | ^7.0",
        "symfony/http-foundation": "^6.2 | ^7.0",
        "symfony/http-kernel": "^6.2 | ^7.0",
        "symfony/intl": "^6.2 | ^7.0",
        "symfony/translation": "^6.2 | ^7.0",
        "symfony/security-core": "^6.2 | ^7.0",
        "twig/intl-extra": "^3.9",
        "twig/extra-bundle": "^3.12",
        "massive/search-bundle": "^2.9"
    },
    "require-dev": {
        "jackalope/jackalope-doctrine-dbal": "^1.3.4",
        "phpunit/phpunit": "^8.0",
        "symfony/browser-kit": "^6.2 | ^7.0",
        "symfony/phpunit-bridge": "^6.2 | ^7.0",
        "symfony/console": "^6.2 | ^7.0",
        "phpspec/prophecy": "^1.17"
    },
    "autoload": {
        "psr-4": {
            "Manuxi\\SuluNewsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Manuxi\\SuluNewsBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "@phpunit"
        ],
        "phpunit": "vendor/bin/phpunit"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true
        }
    }
}
